{
  "compilerOptions": {
    "sourceMap": true,
    "noImplicitAny": false,
    "jsx": "react",
    "module": "amd",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "allowJs": true,
    "moduleResolution": "node",
    "lib": ["es6", "dom"],
    "target": "es5"
  },
  "include": ["_source/scripts/**/*"],
  "exclude": ["node_modules", "**/*.test.ts"]
}
